{
  "name": "react-lego",
  "author": {
    "name": "litten"
  },
  "version": "0.1.0",
  "description": "React project with server-side UI",
  "main": "./lib",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf lib",
    "build:js": "jsx --harmony ./src ./lib",
    "build:jsx": "jsx --harmony -x jsx ./src ./lib",
    "build": "npm run build:js && npm run build:jsx",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "react-lego"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/litten/react-lego.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/litten/react-lego/issues"
  },
  "dependencies": {
    "classnames": "^1.2.0",
    "react-draggable2": "^0.5.1"
  },
  "peerDependencies": {
    "react": ">=0.13",
    "react-tap-event-plugin": ">=0.1.3"
  },
  "devDependencies": {
    "react-tools": "^0.13",
    "browserify": "^7.0.3",
    "reactify": "^0.17.1",
    "react-router": "^0.13"
  }
}
